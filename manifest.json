{
    "name": "Amber Smalltalk",
    "description": "Amberis a Smalltalk that runs on the JS runtime",
    "version": "0.12.6",
    "manifest_version": 2,
    "app": {
        "background": {
            "scripts": [ "background.js" ]
        }
    },
    "sandbox": {
        "pages": ["index.html"]
    },
    "permissions": [
        {"fileSystem": ["write", "retainEntries", "directory"]},
        "storage"
    ],
    "16": "amber-16.png", "128": "amber-128.png" }
}
